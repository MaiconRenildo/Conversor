<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Tabelas</title>
  <style>
    table{
      margin: auto;
    }
    th{
      background-color: rgb(240, 235, 229);
      border: 1px solid black;
      width: 100px;
      text-align: center;
    }
    tr{
      background-color: whitesmoke;
      text-align: center;
    }
  </style>
</head>
<body>
    <table>
      <thead>
        <% header.forEach(col =>{ %>
          <th> <%- col %> </th>
         <% }) %> 
      </thead>
      <tbody>
        <% rows.forEach(row=>{ %>
          <tr>
            <% row.forEach(element=>{ %>
              <td> <%- element %> </td>
            <% }) %> 
          </tr>
        <% }) %>
      </tbody>
    </table>
</body>
</html>
